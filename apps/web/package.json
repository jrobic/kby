{
  "name": "@kby/web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "codegen": "graphql-codegen",
    "start": "serve dist -p 8081",
    "dev": "vite --port 9001 --clearScreen false",
    "prebuild": "npm run codegen",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "TIMING=1 eslint src/**/*.tsx --fix && tsc --noEmit",
    "clean": "rm -rf .turbo dist",
    "test": "jest",
    "test:hook": "jest --lastCommit --silent",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@kby/ui": "workspace:*",
    "graphql-request": "3.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.7"
  },
  "devDependencies": {
    "@kby/graphql": "workspace:*",
    "@kby/scripts": "workspace:*",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@vitejs/plugin-react": "1.1.4",
    "rollup-plugin-visualizer": "5.5.4",
    "serve": "13.0.2",
    "typescript": "4.5.5",
    "vite": "2.7.13",
    "vite-plugin-env-compatible": "1.1.1",
    "vite-plugin-istanbul": "2.4.0",
    "vite-tsconfig-paths": "3.3.17"
  }
}
