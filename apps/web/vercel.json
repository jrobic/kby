{
  "version": 2,
  "scope": "hello159",
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*).(js.map|js|css|png|svg|json)",
      "dest": "/dist/$1.$2"
    },
    { "src": "/(.*)", "dest": "/dist/index.html" }
  ]
}
